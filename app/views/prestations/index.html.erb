<div class="container">
  <div class="row">
    <%= calendar partial: 'simple_calendar/calendar' do |date| %>
      <%= date %>
    <% end %>
  </div>
</div>


<%= month_calendar events: @massage do |date, meetings| %>
  <%= date %>

  <% meetings.each do |meeting| %>
